{
  "compilerOptions": {
    "alwaysStrict": true,
    "declaration": true,
    "downlevelIteration": true,
    "importHelpers": true,
    "inlineSourceMap": true,
    "module": "commonjs",
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "pretty": true,
    "skipLibCheck": true,
    "strictFunctionTypes": true,
    "strictNullChecks": false,
    "target": "es6",
    "lib": ["es6"]
  },
  "exclude": [
    "node_modules"
  ]
}